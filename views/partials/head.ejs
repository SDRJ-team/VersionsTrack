    <style>
        #page_content {
            min-height: 90vh;
        }

        #page_sticky_header {
            width: 100%;
            background-color: rgba(173, 225, 255, 0.86);
            position: fixed;
            padding-top: 8px;
            height: 80px;
            z-index: 2;
        }
        #page_sticky_header.collapse {
            animation: collapse 300ms ease forwards;
        }
        #page_sticky_header.open {
            animation: open 300ms ease forwards;
        }
        #page_sticky_header.home {
            -webkit-transition: background-color 300ms cubic-bezier(0.6, 0.3, 0.8, 0.8);
            transition: background-color 300ms cubic-bezier(0.6, 0.3, 0.8, 0.8);
        }

        .ng-enter {
            -webkit-transition: 1s;
            transition: 1s;
            opacity: 0;
        }

        .ng-enter-active {
            opacity: 1;
        }

        .page-footer {
            padding-top: 20px;
            color: #fff;
            opacity: 1;
        }
        .page-footer .footer-copyright {
            overflow: hidden;
            min-height: 50px;
            display: flex;
            align-items: center;
            text-align: center;
            padding: 0 0;
            color: black;
            background-color: #0000002b;
        }

        .light, .page-footer .footer-copyright {
            font-weight: 100;
        }

        @keyframes collapse {
            from {
                opacity: 1;
                top: 0;
            }
            to {
                opacity: 0;
                top: -64px;
            }
        }
        @keyframes open {
            from {
                opacity: 0;
                top: -64px;
            }
            to {
                opacity: 1;
                top: 0;
            }
        }
    </style>
